{
    "name" : "web_llm",
    "version" : "0.0.1",
    "manifest_version" : 3,
    "description" : "web_llm",
    "icons": {
        "16": "icons8-text-16.png",
        "48": "icons8-text-16.png",
        "128": "icons8-text-16.png"
    },
    "background" : {
        "service_worker" : "background.js"
    },
    "permissions" : [
        "activeTab",
        "tabs"
    ],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon" : {
            "16": "icons8-text-16.png",
            "48": "icons8-text-16.png",
            "128": "icons8-text-16.png"
        }
    },
    "options_page" : "options.html",
    "host_permissions" : [
        "https://*/*"
    ]
}